{
  "name": "movingimage/video-collection-bundle",
  "description": "Symfony bundle that wires the VideoCollection services into the service container based on configuration",
  "type": "library",
  "license": "Proprietary",
  "authors": [
    {
      "name": "Ruben Knol",
      "email": "ruben.knol@movingimage.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.5 || ^7.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "movingimage/video-collection": "^2.0",
    "movingimage/data-provider": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.5 || ^5.0",
    "symfony/expression-language": "*",
    "movingimage/style-checker": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "MovingImage\\Bundle\\VideoCollection\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MovingImage\\Bundle\\VideoCollection\\Tests\\": "Tests/"
    }
  }
}
